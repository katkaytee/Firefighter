void parallel{
  

}

void spin{
  digitalWrite(forward1, HIGH);
  digitalWrite(backward2, HIGH);
  delay(spinTime);

}

void turnLeft{
  digitalWrite(forward1, HIGH);
  digitalWrite(foward2, LOW);
  delay(turnTime);
}

void turnRight{
  digitalWrite(forward2, HIGH);
  digitalWrite(foward1, LOW);
  delay(turnTime);
}

void moveForward{
  digitalWrite(foward1, HIGH);
  digitalWrite(forward2, HIGH);
}
